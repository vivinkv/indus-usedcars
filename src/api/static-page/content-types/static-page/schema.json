{
  "kind": "collectionType",
  "collectionName": "static_pages",
  "info": {
    "singularName": "static-page",
    "pluralName": "static-pages",
    "displayName": "Static Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Page_Heading": {
      "type": "string"
    },
    "Short_Description": {
      "type": "text"
    },
    "Content": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
